<Window x:Class="AdaptivBot.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:AdaptivBot"
        mc:Ignorable="d"
        Title="Warning" Height="200" Width="300">
    <Grid>
        <TextBlock>Username or password different to stored username or password.</TextBlock>
        <StackPanel Orientation="Horizontal">
            <Button Name="btnYes">Yes</Button>
            <Button Name="btnNo">No</Button>
            <Button Name="tbnCancel">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
